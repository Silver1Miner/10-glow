; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "StaticBody2D",
"class": "Furniture",
"language": "GDScript",
"path": "res://src/world/room/Furniture.gd"
}, {
"base": "Area2D",
"class": "Wallpaper",
"language": "GDScript",
"path": "res://src/world/room/Wallpaper.gd"
} ]
_global_script_class_icons={
"Furniture": "",
"Wallpaper": ""
}

[application]

config/name="Glow Worm"
boot_splash/image="res://icon.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.545098, 0.545098, 0.545098, 1 )
config/icon="res://icon.png"
config/macos_native_icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

PlayerData="*res://src/playerdata/PlayerData.tscn"

[display]

window/size/width=640
window/size/height=400
window/size/test_width=640
window/size/test_height=400
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
